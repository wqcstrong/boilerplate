{
  "name": "react-vite-mobile",
  "version": "1.0.0",
  "description": "基于 Vite 构建的 react(mobile) 项目脚手架。",
  "license": "ISC",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix ./src"
  },
  "devDependencies": {
    "@blucass/eslint-config-react": "^1.0.3",
    "@blucass/eslint-config-typescript": "^1.0.3",
    "@types/node": "^17.0.16",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.2.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^7.23.2",
    "less": "^4.1.2",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "typescript": "^4.6.2",
    "vite": "^3.0.4",
    "vite-plugin-svgr": "^1.0.1",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "ahooks": "^3.1.9",
    "antd-mobile": "^5.22.0",
    "dayjs": "^1.10.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "umi-request": "^1.4.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{less,md,json}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx}": [
      "prettier --write"
    ],
    "src/**/*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
