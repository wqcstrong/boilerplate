{
  "name": "react-vite-mobile",
  "version": "1.0.0",
  "description": "基于 Vite 构建的 react(mobile) 项目脚手架。",
  "license": "ISC",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix ./src"
  },
  "devDependencies": {
    "@types/node": "^17.0.16",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^1.2.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.50.0",
    "less": "^4.2.0",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.2.2",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-svgr": "^4.0.0",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "ahooks": "^3.7.8",
    "antd-mobile": "^5.32.4",
    "antd-mobile-icons": "^0.3.0",
    "dayjs": "^1.11.10",
    "eslint-config-alloy": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.16.0",
    "umi-request": "^1.4.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{less,md,json}": [
      "prettier --write"
    ],
    "src/**/*.{js,jsx}": [
      "prettier --write"
    ],
    "src/**/*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
